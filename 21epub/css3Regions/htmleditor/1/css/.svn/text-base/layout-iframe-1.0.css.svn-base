html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
    background: none repeat scroll 0 0 transparent;
    border: 0 none;
    margin: 0;
    outline: 0 none;
    padding: 0;
    vertical-align: baseline;
}
body {
    color: #404040;
    overflow-y: scroll;
}
body {
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-size: 14px;
}
.drsElement {
	cursor: move;
	position: absolute;
}
.drsElement[state~="siteSegment"] {
    cursor: default;
}

.drsMoveHandle {

}

.dragresize {

}

.drsElement-minHeightMark {
    background-image: url("../minimum_line_prt.png");
    background-repeat: repeat-x;
    height: 6px;
    left: 0;
    margin-left: 5px;
    margin-right: 3px;
    position: absolute;
    right: 0;
}
.dragresize-tl {
    height: 20px;
    left: -18px;
    position: absolute;
    top: -18px;./images
    width: 20px;
}
.dragresize-tl .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    left: 13px;
    position: absolute;
    top: 13px;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-tl .knob {
    background-position: 0 -8px;
}
.drsElement[topLeftEnabled="true"] .dragresize-tl {
    cursor: nw-resize;
}
.dragresize-tm {
    background-image: url("../images/bb-default-top.png");
    background-position: center bottom;
    background-repeat: repeat-x;
    height: 10px;
    left: 0;
    position: absolute;
    right: 0;
    top: -10px;
}
.drsElement[state~="masterPage"] .dragresize-tm {
    background-image: url("../images/bb-master-page-top.png");
}
.dragresize-tm .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    left: 50%;
    margin-left: -4px;
    position: absolute;
    top: 5px;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-tm .knob {
    background-position: 0 -8px;
}
.drsElement[topEnabled="true"] .dragresize-tm {
    cursor: n-resize;
}
.dragresize-tr {
    height: 20px;
    position: absolute;
    right: -18px;
    top: -18px;
    width: 20px;
}
.dragresize-tr .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    position: absolute;
    right: 13px;
    top: 13px;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-tr .knob {
    background-position: 0 -8px;
}
.drsElement[topRightEnabled="true"] .dragresize-tr {
    cursor: ne-resize;
}
.dragresize-lm {
	background-image: url("../images/bb-default-left.png");
	background-position: right center;
	background-repeat: repeat-y;
	bottom: 0;
	left: -10px;
	position: absolute;
	top: 0;
	width: 10px;
}
.drsElement[state~="masterPage"] .dragresize-lm {
    background-image: url("../images/bb-master-page-left.png");
}
.dragresize-lm .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    left: 5px;
    margin-top: -4px;
    position: absolute;
    top: 50%;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-lm .knob {
    background-position: 0 -8px;
}
.drsElement[leftEnabled="true"] .dragresize-lm {
    cursor: w-resize;
}

.dragresize-rm {
    background-image: url("../images/bb-default-right.png");
    background-repeat: repeat-y;
    bottom: 0;
    position: absolute;
    right: -10px;
    top: 0;
    width: 10px;
}
.drsElement[state~="masterPage"] .dragresize-rm {
    background-image: url("../images/bb-master-page-right.png");
}
.dragresize-rm .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    margin-top: -4px;
    position: absolute;
    right: 5px;
    top: 50%;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-rm .knob {
    background-position: 0 -8px;
}
.drsElement[rightEnabled="true"] .dragresize-rm {
    cursor: e-resize;
}


.dragresize-bl {
	bottom: -18px;
	height: 20px;
	left: -18px;
	position: absolute;
	width: 20px;
}
.dragresize-bl .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    bottom: 13px;
    height: 8px;
    left: 13px;
    position: absolute;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-bl .knob {
    background-position: 0 -8px;
}
.drsElement[bottomLeftEnabled="true"] .dragresize-bl {
    cursor: sw-resize;
}
.dragresize-bm {
	background-image: url("../images/bb-default-bottom.png");
	background-repeat: repeat-x;
	bottom: -10px;
	height: 10px;
	left: 0;
	position: absolute;
	right: 0;
}
.drsElement[state~="masterPage"] .dragresize-bm {
    background-image: url("../images/bb-master-page-bottom.png");
}
.dragresize-bm .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    height: 8px;
    left: 50%;
    margin-left: -4px;
    position: absolute;
    top: -3px;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-bm .knob {
    background-position: 0 -8px;
}
.drsElement[bottomEnabled="true"] .dragresize-bm {
    cursor: s-resize;
}

.dragresize-bm-pushKnob {
    background-image: url("../images/pusher.png");
    background-position: 0 -21px;
    background-repeat: no-repeat;
    cursor: s-resize;
    height: 21px;
    margin-right: 30px;
    margin-top: -10px;
    position: absolute;
    right: 55%;
    top: -1px;
    width: 23px;
}
.dragresize-bm-pushKnob:hover {
    background-position: 0 -42px;
}
.dragresize-bm-pushKnob:active {
    background-position: 0 -63px;
}

.dragresize-tm-dragHandle {
    background-image: url("../images/smart_drag.png");
    background-position: 0 -21px;
    background-repeat: no-repeat;
    cursor: n-resize;
    height: 21px;
    margin-right: 30px;
    margin-top: -10px;
    position: absolute;
    right: 55%;
    top: 6px;
    width: 23px;
}
.dragresize-tm-dragHandle:hover {
    background-position: 0 -42px;
}
.dragresize-tm-dragHandle:active {
    background-position: 0 -63px;
}

.dragresize-br {
    bottom: -18px;
    height: 20px;
    position: absolute;
    right: -18px;
    width: 20px;
}
.dragresize-br .knob {
    background-image: url("../images/hendles-sprite.png");
    background-repeat: no-repeat;
    bottom: 13px;
    height: 8px;
    position: absolute;
    right: 13px;
    width: 8px;
}
.drsElement[state~="masterPage"] .dragresize-br .knob {
    background-position: 0 -8px;
}
.drsElement[bottomRightEnabled="true"] .dragresize-br {
    cursor: se-resize;
}
.drsElement.knob *:hover {
    background-position: 0 100%;
}

/*widget*/

body, img, p, h1, h2, h3, h4, h5, h6, ul, ol {
		
	list-style: none outside none;
	margin: 0;
	padding: 0;
}
.column {
	float: left;
	height: auto !important;
	min-height: 100px;
	min-width: 100px;
}
.color-blue {
	background: none repeat scroll 0 0 #148EA4;
}
.widget {
	margin:0;
	padding: 2px;
}
.widget .widget-head {
	color: #000000;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	width: 100%;
}
.widget-head a.collapse {
	background: url("../img/buttons.gif") no-repeat scroll -52px 0 transparent;
	display: inline;
	float: left;
	height: 14px;
	margin: 8px 0 8px 4px;
	outline: medium none;
	text-indent: -9999em;
	width: 14px;
}
.color-red h3, .color-blue h3, .color-orange h3 {
	color: #FFFFFF;
}
.color-orange {
	background: none repeat scroll 0 0 #F66E00;
}
.widget .widget-head h3 {
	float: left;
	padding: 0 5px;
}
.widget-head a.remove {
	background: url("../img/buttons.gif") no-repeat scroll -24px 0 transparent;
	display: inline;
	float: right;
	height: 14px;
	margin: 8px 4px 8px 0;
	outline: medium none;
	text-indent: -9999em;
	width: 14px;
}
.widget-head a.edit {
	background: url("../img/buttons.gif") no-repeat scroll 0 0 transparent;
	display: inline;
	float: right;
	height: 14px;
	margin: 8px 4px;
	outline: medium none;
	text-indent: -9999em;
	width: 24px;
}
a {
	color: white;
}
.widget .widget-content {
	color: #DDDDDD;
	line-height: 1.2em;
}
.widget .widget-content p {
	border-bottom: 1px solid #666666;
	padding: 0.8em 0;
}
body { /*整个页面的属性设定*/
	background-color: #CCCCCC; /*背景色*/
	font-family: Geneva, sans-serif; /*可用字体*/
	margin: 10px auto; /*页边空白*/
	max-width: 800px;
	
}
h2 { /*设定整个body内的h2的共同属性*/
	text-align: center; /*文本居中*/
}
header { /*整个body页面的header适用*/
	background-color: #F47D31;
	color: #FFFFFF;
	text-align: center;
}
article { /*整个body页面的article适用*/
	background-color: #eee;
}

nav,article,aside { /*共同属性*/
	margin: 10px;
	padding: 10px;
	display: block;
}
 
header#page_header nav { /*header#page_header nav的属性*/
	list-style: none;
	margin: 0;
	padding: 0;
}
 
header#page_header nav ul li { /*header#page_header nav ul li属性*/
	padding: 0;
	margin: 0 20px 0 0;
	display: inline;
}
 
section#posts { /*#posts 的section属性*/
	display: block;
	float: left;
	width: 70%;
	height: auto;
	background-color: #F69;
}
 
section#posts article footer { /*section#posts article footer属性*/
	background-color: #039;
	clear: both;
	height: 50px;
	display: block;
	color: #FFFFFF;
	text-align: center;
	padding: 15px;
}
 
section#posts aside { /*section#posts aside属性*/
	background-color: #069;
	display: block;
	float: right;
	width: 35%;
	margin-left: 5%;
	font-size: 20px;
	line-height: 40px;
}
 
section#sidebar { /*section#sidebar属性*/
	background-color: #eee;
	display: block;
	float: right;
	width: 25%;
	height: auto;
	background-color: #699;
	margin-right: 15px;
}
 
footer#page_footer { /*footer#page_footer属性*/
	display: block;
	clear: both;
	width: 100%;
	margin-top: 15px;
	display: block;
	color: #FFFFFF;
	text-align: center;
	background-color: #06C;
}
body {
	font-family: Arial,Verdana,Sans-Serif;
	font-size: 0.8em;
}
.widget-content {
	background: url("img/widget-content-bg.png") repeat-x scroll 0 0 #333333;
	color: #DDDDDD;
	line-height: 1.2em;
	overflow: hidden;
	padding: 0 5px;
}